; Adjust the 1.67xdefense_bonus in terrain.ruleset for some attackers: 
; ****************************************************************************
[effect_fortress_nonfortbuster]
type    = "Defend_Bonus"
value   = 20 ;increase 1.67x to 2x except when defending against FortBuster types. 
;note: 1.20*1.67=2.0 (this bonus is multiplied after terrain defense_bonus was multiplied)
reqs    =
    { "type", "name", "range", "present"
      "Extra", "Fortress", "Local", TRUE
      "UnitFlag", "FortBuster", "Local", FALSE
      "UnitFlag", "FortressBuster", "Local", FALSE 
      "UnitFlag", "AirAttacker", "Local", FALSE
    }
[effect_fortressbuster]
type    = "Attack_Bonus"
value   = 67 ;Cancel defender's 1.67x with attacker getting 1.67x 
reqs    =
    { "type", "name", "range", "present"
      "Extra", "Fortress", "Local", TRUE
      "UnitFlag", "FortressBuster", "Local", TRUE 
    }

