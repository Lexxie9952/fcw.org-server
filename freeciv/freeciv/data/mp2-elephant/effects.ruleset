;***************************************************************************************************************************************************************** MOVE COSTS
;By having control over embark/board and disembark/deboard, we can gracefully upgrade mechanics
;to tamp down double-move and transport-swap dynamics while still leaving a little flex in
;smoothing logistics and transportation issues via transportation bonnses instead of OP 3x
;movement:
;This now makes Paratroopers like Marines but see note in actionenabler below
[effect_action_success_move_cost_board]
type    = "Action_Success_Actor_Move_Cost"
value   = 3360
reqs    =
    { "type",          "name",            "range", "present"
      "Action",        "Transport Board", "Local", TRUE
      "UnitClassFlag", "DelayLoad",       "Local", TRUE      ; Does not affect Aircraft or ships
      "UnitType",      "Marines",         "Local", FALSE     ; Does not affect Marines
      "UnitType",      "Paratroopers",    "Local", FALSE     ; Does not affect Paratroopers
      "UnitClassFlag", "UnloadOnce",      "Local", FALSE     ; prevent 2/3 TiredAttack by loading up a Bomb; inability to uses nuke-bombs after loading them.
    }

;note this allows Paratroopers to deboard for 0 like Marines but since we're getting
;rid of deboarding costs because of passive movement anyway, not a big deal. we're only
;stalled on doing this because not yet figured out how it affects Bombs but perhaps
;they're not DelayLoad or perhaps we just make this only apply to Bombs but better yet
;is to just rethink Bombs under the new system.
[effect_action_success_move_cost_deboard]
type    = "Action_Success_Actor_Move_Cost"
value   = 3360
reqs    =
    { "type",          "name",              "range", "present"
      "Action",        "Transport Deboard", "Local", TRUE
      "UnitClassFlag", "DelayLoad",         "Local", TRUE      ; Does not affect Aircraft or ships
      "UnitType",      "Marines",           "Local", FALSE     ; Does not affect Marines
      "UnitType",      "Paratroopers",      "Local", FALSE     ; Doesn't affect Paratroopers
    }
[effect_action_success_move_cost_deboard_to_unsupported_tile_is_a_disembark_1]    ; TO DO: If we move all deboard illegality rules to this section we can liberalize deboarding rules in client and here too.
type    = "Action_Success_Actor_Move_Cost"
value   = 999999  ; same as disembark: lose all moves
reqs    =
    { "type",          "name",              "range", "present"
      "Action",        "Transport Deboard", "Local", TRUE
      "UnitClassFlag", "TerrainSpeed",      "Local", TRUE      ; Only affects land units, ...
      "UnitType",      "Marines",           "Local", FALSE     ; except Marines
      "UnitType",      "Paratroopers",      "Local", FALSE     ; except Paratroopers from Cargo Plane

      ; For now since this is one effect for ALL land units, exclude ANY tile that ANY unit can legally deboard to:
      "CityTile",      "Center",            "Local", FALSE
      "Extra",         "Quay",              "Local", FALSE
      "Extra",         "Hideout",           "Local", FALSE
      "Extra",         "Fort",              "Local", FALSE
      "Extra",         "Fortress",          "Local", FALSE
      "Extra",         "Naval Base",        "Local", FALSE
      "Extra",         "Castle",            "Local", FALSE
      "Extra",         "Bunker",            "Local", FALSE
      "Extra",         "Airbase",           "Local", FALSE
    }
; See notes 2 above for how this affects Paratroopers and makes them like Marines
[effect_action_success_move_cost_transport_unload]
type    = "Action_Success_Target_Move_Cost"   ; "Local" scope on unit pertains to target, not actor.
value   = 3360
reqs    =
    { "type",          "name",             "range", "present"
      "Action",        "Transport Unload", "Local", TRUE
      "UnitClassFlag", "DelayLoad",        "Local", TRUE      ; Does not affect Aircraft or ships
      "UnitType",      "Marines",          "Local", FALSE     ; Does not affect Marines
      "UnitType",      "Paratroopers",     "Local", FALSE     ; Does not affect Paratroopers
    }
[effect_action_success_move_cost_transport_unload_to_unsupported_tile_is_a_disembark_1]    ; TO DO: If we move all unload illegality rules to this section we can liberalize unloading rules in client and here too.
type    = "Action_Success_Target_Move_Cost"
value   = 999999  ; same as disembark: lose all moves
reqs    =
    { "type",          "name",              "range", "present"
      "Action",        "Transport Unload",  "Local", TRUE
      "UnitClassFlag", "TerrainSpeed",      "Local", TRUE      ; Only affects land units, ...
      "UnitType",      "Marines",           "Local", FALSE     ; except Marines

      ; For now since this is one effect for ALL land units, exclude ANY tile that ANY unit can legally deboard to:
