;###############################################
;Bombs can't be used after a double-move double-cargo run, so unloading has to ensure half of all moves are gone
;so the Bomb wouldn't be able to load up after two cargo hauls. Nukes have 1 1/3 moves (to allow disembark),
;and conventional have 1 move.
[effect_action_success_move_cost_unload_bombs]
type    = "Action_Success_Target_Move_Cost"   ; "Local" scope on unit pertains to target, not actor.
value   = 3360                                ; 20+20 = 2/3, which is double cost for unload since it has no cost for load, and keeps
reqs    =                                     ; both Bomb and Bomb2 classes only able to do one cargo haul.
    { "type",          "name",             "range", "present"
      "Action",        "Transport Unload", "Local", TRUE
      "UnitClassFlag", "UnloadOnce",       "Local", TRUE
    }
[effect_action_success_move_cost_deboard_bombs]
type    = "Action_Success_Actor_Move_Cost"    ; "Local" scope on unit pertains to target, not actor.
value   = 3360                                  ; 20+20 = 2/3, which is double cost for unload since it has no cost for load, and keeps
reqs    =                                     ; both Bomb and Bomb2 classes only able to do one cargo haul.
    { "type",          "name",              "range", "present"
      "Action",        "Transport Deboard", "Local", TRUE
      "UnitClassFlag", "UnloadOnce",        "Local", TRUE
    }

