[effect_medici_gold]
type    = "Output_Bonus_2"
; Output_Bonus_2 uses trunc rounding. True +7% is achieved knowing the following:
; 8% with trunc rounding: xÌ„ = +7.06% gold. (median is slightly lower)
; 7% with trunc rounding: xÌ„ = +5.96% gold. (median is lower)
; (NB: bonus suffers somewhat more under lower gold tax rates)
; If changing from 7% to another value, use "Medici Rounding.ods" to calibrate
value	= 8
reqs    =
    { "type",       "name",        "range", "present"
      "Building",   "Medici Bank", "Player", TRUE
      "Building",   "Bank",        "City",   TRUE
      "OutputType", "Gold",        "Local",  TRUE
      "Gov",        "Communism",   "Player", FALSE
    }
