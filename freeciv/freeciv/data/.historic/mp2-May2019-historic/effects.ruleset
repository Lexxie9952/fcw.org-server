    { "type", "name", "range", "present"
      "Building", "Recycling Center", "City", TRUE
      "Building", "Solar Plant", "City", TRUE
      "Building", "Factory", "City", TRUE
      "Building", "Mfg. Plant", "City", FALSE
    }

